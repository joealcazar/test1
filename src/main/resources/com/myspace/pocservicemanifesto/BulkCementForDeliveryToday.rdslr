package com.myspace.pocservicemanifesto;

import java.lang.Number;

rule "BulkCementForDeliveryToday"
	dialect "mvel"
	ruleflow-group "ValidDates"
	when
		The difference between the created date time and the requested date time is 1 days, the shipping condition selected for the Order is "Delivery" and the distance between JobSite and Plant is more than 3500
		The load includes product type "Bulk Cement"
	then
		Add 1 days to Requested Date
		Log rule name
end
